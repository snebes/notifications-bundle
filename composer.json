{
    "name": "snebes/notifications-bundle",
    "description": "This bundle aims to provide a centralized notification component for Symfony",
    "type": "symfony-bundle",
    "keywords": ["Symfony", "bundle", "notifications", "email", "sms", "doctrine"],
    "homepage": "https://github.com/snebes/notifications-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Steve Nebes",
            "email": "steve@nebes.net"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "snebes/notifications": "*",
        "symfony/framework-bundle": "~3.4|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5|^8.0",
        "doctrine/orm": "^2.6"
    },
    "config": {
        "bin-dir": "bin",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "SN\\Bundle\\NotificationsBundle\\": "src/"
        }
    }
}
