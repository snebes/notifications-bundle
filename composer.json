{
    "name": "snebes/notifications-bundle",
    "description": "Unofficial Symfony Notifications Bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Steve Nebes",
            "email": "steve@nebes.net"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "snebes/notifications": "*@dev",
        "symfony/framework-bundle": "^3.4|^4.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "SN\\Bundle\\NotificationsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SN\\Bundle\\NotificationsBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
